export async function getStaticPaths() {
  const { default: apps } = await import(
    "../../../bincache/aarch64-linux/_apps.json"
  );

  return apps.map((app) => ({
    params: {
      slug: `${app.pkg_family || "community"}/${app.pkg_name || app.pkg}`,
    },
    props: {
      data: app,
    },
  }));
}

const { data } = Astro.props;
